<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    xmlns:tools="http://schemas.android.com/tools"
    package="team.applemango.runnerbe.feature.register.snslogin">

    <uses-permission android:name="android.permission.INTERNET" />

    <dist:module
        dist:instant="false"
        dist:title="@string/dist_dfm_register_snslogin">
        <dist:fusing dist:include="true" />
        <dist:delivery>
            <dist:install-time />
            <dist:removable value="true" />
        </dist:delivery>
    </dist:module>

    <application tools:ignore="MissingApplicationIcon">
        <activity
            android:name=".SnsLoginActivity"
            android:theme="@style/Theme.RunnerBe" />

        <activity-alias
            android:name="team.applemango.runnerbe.util.DFMLoginActivityAlias"
            android:targetActivity=".SnsLoginActivity"
            android:theme="@style/Theme.RunnerBe" />
    </application>

</manifest>